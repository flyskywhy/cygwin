    <listitem><link linkend="module_CheckLibraryExists"><emphasis><literal>CheckLibraryExists</literal></emphasis></link></listitem>
    <listitem><link linkend="module_CheckStructHasMember"><emphasis><literal>CheckStructHasMember</literal></emphasis></link></listitem>
    <listitem><link linkend="module_CheckSymbolExists"><emphasis><literal>CheckSymbolExists</literal></emphasis></link></listitem>
    <listitem><link linkend="module_CheckTypeSize"><emphasis><literal>CheckTypeSize</literal></emphasis></link></listitem>
