    <listitem><link linkend="module_CTest"><emphasis><literal>CTest</literal></emphasis></link></listitem>
    <listitem><link linkend="module_CheckCCompilerFlag"><emphasis><literal>CheckCCompilerFlag</literal></emphasis></link></listitem>
    <listitem><link linkend="module_CheckCSourceCompiles"><emphasis><literal>CheckCSourceCompiles</literal></emphasis></link></listitem>
